{
    "name": "jenky/restful-eloquent",
    "type": "library",
    "description": "RESTful service with Laravel Eloquent",
    "keywords": [
        "rest",
        "restful",
        "laravel",
        "eloquent"
    ],
    "homepage": "https://github.com/jenky/restful-eloquent",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "jenky.w0w@gmail.com",
            "homepage": "https://github.com/jenky",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\RESTfulEloquent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\RESTfulEloquent\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
